{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "jsx": "preserve",
    "skipLibCheck": true,
    "lib": [
      "ES2018"
    ]
  },
  "paths": {
    "@root/*": [
      "/*"
    ],
    "@components/*": [
      "src/components/*"
    ],
    "@assets/*": [
      "src/assets/*"
    ],
    "@layouts/*": [
      "src/layouts/*"
    ]
  }
}